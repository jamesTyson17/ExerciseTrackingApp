<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Tyson.android.wt"
    android:versionCode="1"
    android:versionName="4.0" >

    <uses-sdk android:minSdkVersion="11" />

    <application
        android:name=".app.ExerciseTrackerApp"
        android:icon="@drawable/start_icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light" >
        <activity
            android:name=".activities.ShowListOfExercisesActivity"
            android:configChanges="orientation|keyboard"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.EntryListActivity" >
        </activity>
        <activity android:name=".activities.EntryDateActivity" >
        </activity>
        <activity android:name=".activities.EntryGraphActivity" >
        </activity>
        <activity android:name=".activities.EntryStatsActivity" >
        </activity>
        <activity android:name=".activities.EntryStopWatch" >
        </activity>
        <activity
            android:name=".activities.TabWidget"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Holo.Light" >
        </activity>
        
    </application>

</manifest>